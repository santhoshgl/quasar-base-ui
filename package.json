{
  "name": "base-ui",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar Starter App",
  "author": "Jugal <jugaldeepak@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "generate:favicon:spa": "icongenie generate -i favicon.png -m spa --skip-trim",
    "debug:web": "quasar dev",
    "debug:ios": "quasar dev -m capacitor -T ios",
    "debug:android": "quasar dev -m capacitor -T android",
    "debug:prod:ios": "quasar build -m capacitor -T ios -d --ide",
    "debug:prod:android": "quasar build -m capacitor -T android -d --ide"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.5",
    "axios": "^0.27.2",
    "axios-auth-refresh": "^3.3.4",
    "core-js": "^3.6.5",
    "dayjs": "^1.11.6",
    "firebase": "^9.12.1",
    "pinia": "^2.0.13",
    "qs": "^6.11.0",
    "quasar": "^2.10.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "eslint": "^8.10.0",
    "dotenv": "^16.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.5.1",
    "@quasar/app-webpack": "^3.0.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
